<form id="rating-form" method="post">
	<ul id="star-group">
		<div class="login-notice"></div>
		<li class="stars">
			<label for="rating_1"><i class="fa fa-star" aria-hidden="true"></i></label>
			<input type="radio" name="ratings" id="rating_1" value="1"/>
		</li>
		<li class="stars">
			<label for="rating_2"><i class="fa fa-star" aria-hidden="true"></i></label>
			<input type="radio" name="ratings" id="rating_2" value="2"/>
		</li>
		<li class="stars">
			<label for="rating_3"><i class="fa fa-star" aria-hidden="true"></i></label>
			<input type="radio" name="ratings" id="rating_3" value="3"/>
		</li>
		<li class="stars">
			<label for="rating_4"><i class="fa fa-star" aria-hidden="true"></i></label>
			<input type="radio" name="ratings" id="rating_4" value="4"/>
		</li>
		<li class="stars">
			<label for="rating_5"><i class="fa fa-star" aria-hidden="true"></i></label>
			<input type="radio" name="ratings" id="rating_5" value="5"/>
			
		</li>
	</ul>
	<div id="review-textbox">
		<textarea id="review-area" name="review-area"></textarea> 
		<button id="rating-submit">Submit</button>
		<input type="hidden" id="location_Id" name="locationId" value=<%= data[0].camis %>  />
		<input type="hidden" id="star-dba" name="dba" value=<%= data[0].dba %>  />
        <input type="hidden" id="star-building" name="building" value=<%= data[0].building %>  />
        <input type="hidden" id="star-street" name="street" value=<%= data[0].street %>  />
        <input type="hidden" id="star-boro" name="boro" value=<%= data[0].boro %>  />
        <input type="hidden" id="star-zipcode" name="zipcode" value=<%= data[0].zipcode %>  />
        <input type="hidden" id="userId" name="userId" value=1  />
</div>

</form>